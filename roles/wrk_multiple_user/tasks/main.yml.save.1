#  tasks file 
- name: create user profile
  ibmi_user_and_group:
    operation: 'create'
    user: "{{item}}"
    text: Usuario "{{ item }}" creado por ansible 
    user_class: "{{ clase_usuario }}"
    expire: '*YES'
#    parameters: 
#     LANGID: ESP
#      CNTRYID: ES
#      CCSID: 284
  loop: "{{usuarios}}"
  register: resultado

- name: resultado de la operacion
  debug:
    var: "{{resultado}}"
